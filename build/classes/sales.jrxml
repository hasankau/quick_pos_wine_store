	<parameter name="from" isForPrompting="true" class="java.lang.String"/>
	<parameter name="to" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
     sum(invoice_details.`qty`)AS invoice_details_qty,
     item.`barcode` AS item_barcode,
     item.`name` AS item_name,
     item.`manufacturer` AS item_manufacturer,
     invoice.`date` AS invoice_date
FROM
     `item` item 
     INNER JOIN `invoice_details` invoice_details ON item.`item_code` = invoice_details.`Item_item_code`
     INNER JOIN `invoice` invoice ON invoice_details.`invoice_invoice_id` = invoice.`invoice_id`
where
	date(invoice.`date`) between $P{from} and $P{to}
group by item_barcode]]></queryString>

